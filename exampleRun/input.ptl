#define numPoints 64
#define twoPi 6.28318530718
Grid
{
    blockDim  = [2, 2, 2]
    blockSize = [$(numPoints), $(numPoints), $(numPoints)]
    nguard = 3
}
Fluid
{
    gamma = 1.4
    cp = 3.5
    visc = @choose(1, 0.000625, 1.0)
    Tinf = 0.7143
    prandtl = 0.72
}
TGV
{
    reynolds = 1600.0
    mach = 0.1
    L = @div(1.0, $(twoPi))
}
Time
{
    numSteps = 30000
    timestep = 0.0002
}
Output
{
    outputInterval = 1000
    doMMS = true
}
Stats
{
    outputKineticEnergy = true
}
